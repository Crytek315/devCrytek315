<meta charset="UTF-8">
<script>
    var pet = prompt('Numerator');
    var vas = prompt('Denominator');
    var n = 0;
    do {
        n++;
        if (n >= pet || n >= vas) {
            alert('You can't reduce this fraction');
            break;
        }
        else if(pet % n === 0 && vas % n === 0 && n != 1) {
            alert('You can reduce this fraction');
            break;
        }


    } while (n < pet  || n < vas );


</script>
